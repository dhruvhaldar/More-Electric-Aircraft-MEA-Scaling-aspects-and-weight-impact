@article{Zhang1994,
abstract = {A standard $\kappa$-$\epsilon${lunate} model is used to simmulate the turbulent recirculating flows over a backward facing step, a forward facing step and a single block. The advection terms are discretized by the upwind weighted difference scheme, the pressure problem is resolved by the artificial compressibility method. The results show that the $\kappa$-$\epsilon${lunate} model can give a good prediction for the mean velocities and the static pressure field. The turbulent kinetic energy is underpredicted. The numerical errors can produce serious deficiencies in the predictions. The structure of turbulent recirculating flows over a backward facing step is different to that over a forward facing step. Finally, the model is applied to the flow over a surface-mounted cube placed in a turbulent boundary layer. {\textcopyright} 1994.},
author = {Zhang, C. X.},
