keywords = {Aeronautical,Air conditioning,Air cycle machine,Air recirculation,Commercial aircraft,Environmental control systems,Refrigeration,Vapor compression cycle,Water separation},
number = {1274},
pages = {499--532},
title = {{Air conditioning systems for aeronautical applications: A review}},
volume = {124},
year = {2020}
}
@book{HermanV.CottonyandRichardS.Dill1941,
